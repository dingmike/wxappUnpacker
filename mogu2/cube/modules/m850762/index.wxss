@import "../../../cube-core/componentClass/internalComponents/mvwImage/index.wxss";

.MCUBE_MOD_ID_850762 .long-cut {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.MCUBE_MOD_ID_850762 .signIn {
    position: relative;
    display: flex;
    flex-direction: column;
    background-size: 100% auto;
    background-repeat: no-repeat;
    background-position: left bottom;
}

.MCUBE_MOD_ID_850762 .signIn-title {
    width: 100%;
    height: 45rpx;
    line-height: 45rpx;
    margin-bottom: 14rpx;
    font-size: 32rpx;
    text-align: center;
}

.MCUBE_MOD_ID_850762 .signIn-btn {
    width: 100%;
    flex-shrink: 0;
    flex-grow: 0;
    text-align: center;
    font-size: 26rpx;
}

.MCUBE_MOD_ID_850762 .signIn-un-btn {
    height: 60rpx;
    line-height: 60rpx;
}

.MCUBE_MOD_ID_850762 .signIn-ed-btn {
    height: 52rpx;
    line-height: 52rpx;
}

.MCUBE_MOD_ID_850762 .signIn-btn-box {
    display: inline-block;
    height: 100%;
    max-width: 80%;
    border-radius: 6rpx;
    border-width: 1rpx;
    border-style: solid;
}

.MCUBE_MOD_ID_850762 .signIn-ed-btn-box {
    padding: 0 22rpx;
}

.MCUBE_MOD_ID_850762 .signIn-un-btn-box {
    padding: 0 55rpx;
}