.component__launchapp {
    width: 100%;
    height: 68rpx;
    padding: 0 30rpx;
    margin-bottom: 18rpx;
    background-size: contain;
    background-repeat: no-repeat;
}

.component__launchapp--close {
    font-size: 20rpx;
    color: #fff;
    font-weight: bold;
}

.component__launchapp--btn {
    width: 108rpx;
    height: 42rpx;
    font-size: 24rpx;
    color: #fff;
    background: #FF5777;
    border-radius: 6rpx;
}