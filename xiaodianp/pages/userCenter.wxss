page {
    background-color: #f4f4f4;
    height: 100%;
}

.userinfo {
    display: -webkit-box;
    display: flex;
    padding: 30rpx;
    -webkit-box-align: center;
    align-items: center;
    background-color: #fff;
    padding-bottom: 80rpx;
    border-bottom: 20rpx solid #f4f4f4;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.userNameView {
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: top;
    align-items: top;
    background-color: #fff;
}

.userinfo-avatar {
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    display: -webkit-box;
    display: flex;
    -webkit-box-pack: start;
    justify-content: flex-start;
    font-size: 50rpx;
    color: #000;
    font-weight: 900;
    padding-bottom: 10rpx;
}

.userinfo-sign {
    font-size: 28rpx;
    color: #999999;
}

.cellView {
    height: 100rpx;
    border-bottom: 2rpx solid #f4f4f4;
    background-color: #fff;
}

.cellView .cell {
    padding: 30rpx;
}

.cellView .cell image {
    margin-right: 10rpx;
    width: 40rpx;
    height: 40rpx;
}

.cellView .cell text {
    font-size: 28rpx;
    line-height: 32rpx;
    letter-spacing: 2rpx;
    color: #545454;
    margin-right: 10rpx;
    display: -webkit-box;
}

.cellView .cell .arrow {
    position: relative;
}

.cellView .cell .arrow:after {
    content: ' ';
    height: 12rpx;
    width: 12rpx;
    border-width: 4rpx 4rpx 0 0;
    border-color: #c8c8cd;
    border-style: solid;
    transform: matrix(.71,.71,-0.71,.71,0,0);
    top: -4rpx;
    position: absolute;
    top: 50%;
    margin-top: -8rpx;
    right: 4rpx;
}

.contact {
    line-height: 25px;
    border: 0;
    text-align: left;
    background-color: #fff;
}

.contact::after {
    content: none;
}

.applyBtn {
    border: 1px solid #fff;
    border-radius: 45rpx;
    display: -webkit-box;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    margin: 50rpx auto;
    width: 600rpx;
    height: 80rpx;
    background-color: #fff;
}

.applyBtn text {
    font-size: 28rpx;
    line-height: 32rpx;
    letter-spacing: 2rpx;
    color: #545454;
    margin-right: 10rpx;
    display: -webkit-box;
}

.applyBtn-content {
    margin: 0 auto;
}

.partner {
    text-align: center;
}

.partner .tencent-cooperation {
    width: 550rpx;
    margin: 0 auto 60rpx;
}

.bottom {
    border-bottom: 20rpx solid #f4f4f4;
}