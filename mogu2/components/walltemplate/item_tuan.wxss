#wall_nav_box {
    font-family: "Helvetica Neue","Hiragino Sans GB W3","Hiragino Sans GB","Microsoft YaHei","WenQuanYi Micro Hei",sans-serif;
}

.clearfix:before,.clearfix:after {
    content: "";
    display: table;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    zoom: 1;
}

.userselect {
    -webkit-user-select: text;
}

.text_hide {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    width: 100%;
}

.J_items_box .ui-empty {
    position: relative;
    display: block;
    width: 100%;
    height: 0;
    padding-bottom: 100%;
    overflow: hidden;
}

.J_items_box .ui-empty-box {
    position: absolute;
    display: flex;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.J_items_box .ui-empty-image {
    width: 514rpx;
    height: 260rpx;
    background: transparent no-repeat;
    background-size: cover;
}

.J_items_box .ui-empty-text {
    margin-top: 30rpx;
    color: #666;
    font-size: 32rpx;
}

.J_items_box .ui-empty-tip {
    margin-top: 20rpx;
    color: #666;
    font-size: 24rpx;
}

.padding_box {
    height: 0;
    overflow: hidden;
    position: relative;
}

.J_items_box .J_dynamic_imagebox {
    background-color: #e5e5e5;
}

.J_scroll_wallbox .fl,.J_items_box .fl {
    float: left;
}

.J_scroll_wallbox .fr,.J_items_box .fr {
    float: right;
}

.J_items_box .goods_item {
    position: relative;
    display: block;
    height: 610rpx;
    width: 348rpx;
    margin: 18rpx 0 0 18rpx;
    background: #fff;
    border-radius: 6rpx;
    overflow: hidden;
}

.J_items_box .goods_item .goods_img {
    height: 462rpx;
    border-radius: 6rpx 6rpx 0 0;
    overflow: hidden;
}

.J_items_box .goods_item img {
    width: 100%;
    display: block;
}

.J_items_box .goods_item .top_icon {
    position: absolute;
    top: 0;
    left: 7rpx;
    height: 140rpx;
    width: 66rpx;
    z-index: 3;
}

.J_items_box .goods_item .goods_info {
    padding: 12rpx 18rpx 0 18rpx;
}

.J_items_box .goods_item .name_box {
    height: 74rpx;
    overflow: hidden;
}

.J_items_box .goods_item .goods_name {
    width: 100%;
    line-height: 34rpx;
    max-height: 67rpx;
    font-size: 24rpx;
    color: #333;
    overflow: hidden;
}

.J_items_box .goods_item .tags {
    display: none;
}

.J_items_box .goods_item .name_box_hastag .goods_name {
    line-height: 34rpx;
    height: 33rpx;
}

.J_items_box .goods_item .name_box_hastag .tags {
    display: block;
}

.J_items_box .goods_item .name_box_hastag .tags img {
    height: 32rpx;
    width: auto;
    margin: 5rpx 4rpx 0 0;
}

.J_items_box .goods_item .p_price {
    max-width: 45%;
    margin-top: 12rpx;
    height: 34rpx;
    line-height: 34rpx;
    font-size: 30rpx;
    font-weight: bold;
    color: #333;
    overflow: hidden;
}

.J_items_box .goods_item .p_feed {
    max-width: 45%;
    height: 34rpx;
    line-height: 34rpx;
    margin-top: 14rpx;
    font-size: 22rpx;
    color: #999;
    overflow: hidden;
}

.J_items_box .goods_item_v61 .like_link {
    position: absolute;
    right: 0;
    top: 376rpx;
    width: 86rpx;
    height: 86rpx;
    background: url(https://s10.mogucdn.com/mlcdn/c45406/170818_699522ajhfbk0h33568j7j8b2l3bb_87x87.png) no-repeat center center;
    background-size: 58rpx 58rpx;
}

.goods_info {
    font-family: "PingFang SC";
}

.goods_title {
    width: 100%;
    font-size: 24rpx;
    min-height: 34rpx;
    height: 34rpx;
    color: #333;
}

.good_title_discount {
    float: left;
    height: 100%;
    line-height: 32rpx;
    padding: 0 10rpx;
    font-size: 18rpx;
    color: #9A0000;
    border: 1rpx solid #9A0000;
    margin-right: 10rpx;
}

.goods_price_old {
    width: 100%;
    height: 28rpx;
    font-size: 24rpx;
    color: #999999;
    margin-top: 10rpx;
    text-decoration: line-through;
    margin-bottom: 6rpx;
}

.goods_price {
    height: 48rpx;
    color: #666;
    position: relative;
}

.goods_price_now {
    position: absolute;
    top: 0;
    left: 0;
    font-size: 36rpx;
    font-weight: 700;
    color: #9A0000;
}

.goods_sale {
    position: absolute;
    right: 0;
    top: 10rpx;
    max-width: 50%;
    height: 44rpx;
    line-height: 44rpx;
    color: #666;
    font-size: 24rpx;
}

.wall_loading {
    text-align: center;
    font-size: 30rpx;
    padding: 20rpx 0;
    color: #888;
}