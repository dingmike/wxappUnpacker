.swiper-cart,.swiper-coupon {
    width: 750rpx;
    height: 100%;
}

.swiper-cart {
    position: Â relative;
}

.swiper-coupon {
    position: absolute;
    transform: translate(100%,0%) translateZ(0);
}

.swiper-cart-true {
    transform: translate(-100%,0%) translateZ(0);
    animation: cartLeft 200ms;
}

.swiper-coupon-true {
    transform: translate(0%,0%) translateZ(0);
    animation: couponLeft 200ms;
}

.swiper-cart-false {
    transform: translate(0%,0%) translateZ(0);
    animation: cartBack 200ms;
}

.swiper-coupon-false {
    transform: translate(100%,0%) translateZ(0);
    animation: couponBack 200ms;
}

