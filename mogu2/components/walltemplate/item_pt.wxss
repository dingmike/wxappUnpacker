.wallPtMore-wrap {
    width: 100%;
    height: 100rpx;
    padding: 0 32rpx;
    overflow: hidden;
    background: #fff;
    border-bottom: 1rpx solid #ddd;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.wallPtMore-text {
    font-size: 28rpx;
    color: #444;
}

.wallPtMore-right {
    font-size: 24rpx;
    color: #999;
    margin-right: 4rpx;
}

.wallPtMore-icon {
    font-size: 26rpx;
    color: #999;
}

.ptWall-item {
    position: relative;
    width: 100%;
    height: 268rpx;
    background: #fff;
    border-bottom: 1rpx solid #efefef;
    padding: 24rpx;
    overflow: hidden;
}

.ptWall-item-image-wrap {
    position: relative;
    float: left;
    height: 220rpx;
    width: 220rpx;
    margin-right: 23rpx;
    background: #efefef;
    border-radius: 6rpx;
    overflow: hidden;
}

.ptWall-item-image {
    width: 100%;
    height: 100%;
}

.ptWall-item-normal {
    position: absolute;
    top: 0;
    left: 0;
    max-width: 180rpx;
    height: 36rpx;
    line-height: 36rpx;
    padding: 0 10rpx;
    opacity: 0.9;
    background: #f55;
    border-radius: 6rpx 0 6rpx 0;
    color: #fff;
    font-size: 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.ptWall-item-new {
}

.ptWall-item-info {
    width: 460rpx;
    height: 220rpx;
    overflow: hidden;
}

.ptWall-item-info-title {
    width: 100%;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.ptWall-item-info-descr {
    width: 340rpx;
    height: 33rpx;
    line-height: 33rpx;
    font-size: 24rpx;
    color: #666;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin: 4rpx 0 15rpx 0;
}

.ptWall-item-info-price {
    position: absolute;
    bottom: 24rpx;
    width: 270rpx;
    height: 50rpx;
    font-size: 48rpx;
    color: #999;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 50rpx;
}

.ptWall-item-info-price-tuanPrice-dollar {
    font-size: 24rpx;
    color: #ff5777;
}

.ptWall-item-info-price-tuanPrice {
    font-size: 48rpx;
    color: #ff5777;
}

.ptWall-item-info-price-oldPrice {
    font-size: 24rpx;
    color: #999;
    margin-left: 6rpx;
    text-decoration: line-through;
}

.ptWall-item-info-tag {
    position: absolute;
    max-width: 260rpx;
    height: 33rpx;
    line-height: 33rpx;
    font-size: 24rpx;
    color: #ff5777;
    border-radius: 4rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}

.ptWall-item-info-tag-tuanInfo {
    max-width: 260rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.ptWall-item-info-tag-tuanAvatarItem {
    width: 32rpx;
    height: 32rpx;
    border-radius: 32rpx;
    border: 1px solid #fff;
    margin-left: -10rpx;
    flex-shrink: 0;
    overflow: hidden;
}

.ptWall-item-info-tag-tuanAvatarList {
    margin-left: 10rpx;
}

.ptWall-item-info-btn {
    position: absolute;
    bottom: 24rpx;
    right: 24rpx;
    width: 180rpx;
    height: 66rpx;
    line-height: 66rpx;
    font-size: 28rpx;
    text-align: center;
    border-radius: 6rpx;
    border: 1px solid #ff5777;
    overflow: hidden;
}

.ptWall-item-info-btn-ing {
    color: #fff;
    background-image: linear-gradient(109deg,#ff8b5e,#ff5777);
    border: none;
}

.ptWall-item-info-btn-out {
    color: #ff5777;
    background: #fff;
}

.ptWall-item-info-soldOutImage {
    position: absolute;
    top: 0;
    right: 0;
    width: 116rpx;
    height: 140rpx;
    overflow: hidden;
}

.wall_nav_box {
    width: 100%;
    max-width: 750px;
    height: 90rpx;
    line-height: 90rpx;
}

.wall_nav_box .nav_middle {
    position: relative;
    z-index: 10;
    height: 90rpx;
    width: 100%;
    max-width: 750px;
    background: #fff;
    border-bottom: 1rpx solid #efefef;
}

.wall_nav_box .nav_fixed {
    position: fixed;
    top: 0;
    z-index: 80;
}

.wall_nav_top_sticky {
    position: sticky;
    position: -webkit-sticky;
    top: 0;
    z-index: 80;
}

.wall_nav_box .nav_tabs_scroll {
    width: 100%;
    height: 90rpx;
    overflow: hidden;
    position: relative;
}

.wall_nav_box .scroll_box {
    width: 100%;
    height: 110rpx;
    padding: 0 10rpx;
    white-space: nowrap;
    overflow: scroll;
    overflow-y: hidden;
    font-size: 0;
    overflow-scrolling: touch;
    -webkit-text-size-adjust: none;
    text-align: center;
}

.wall_nav_box .nav_tabs_scroll .nav_tab {
    display: inline-block;
    padding: 0 22rpx;
    height: 90rpx;
}

.wall_nav_box .nav_tabs_scroll .nav_tab .t_text {
    font-size: 28rpx;
    color: #fff;
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 14rpx;
}

.wall_nav_box .nav_tabs_scroll .tab_on .t_text {
    border-bottom: 4rpx solid #fff;
}

.wall_nav_box .nav_tabs_scroll .tab_on .t_text,.wall_nav_box .tab_on .t_icon {
    border-bottom: 4rpx solid #ff5777;
}

.wall_nav_box .nav_tabs_scroll .nav_tab .t_text {
    color: #666;
}

.wall_nav_box .nav_tabs_scroll .tab_on .t_text {
    color: #ff5777;
}

.wall_loading {
    text-align: center;
    font-size: 30rpx;
    padding: 20rpx 0;
    color: #888;
}