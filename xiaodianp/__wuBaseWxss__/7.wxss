/*! Import by _C[11], whose real path we cannot found. */
.upload {
    width: 100%;
    height: 120rpx;
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    margin-bottom: 20rpx;
}

.upload .uploadImage {
    width: 60rpx;
    height: 60rpx;
    animation: start 1s linear infinite;
    -webkit-animation: start 1s linear infinite;
    -ms-animation: start 1s linear infinite;
    margin-right: 10rpx;
}

.upload text {
    font-size: 26rpx;
}

@-webkit-keyframes start {
    0%,100% {
        -webkit-transform: rotate(0deg);
    }

    100% {
        -webkit-transform: rotate(360deg);
    }
}
/*! Import end */
.item-container {
    display: -webkit-box;
    display: flex;
    flex-wrap: wrap;
    padding: 30rpx 22rpx 0;
}

.nav-box {
    width: 100%;
    height: 80rpx;
    overflow-x: auto;
    overflow-y: hidden;
    z-index: 50;
    background-color: #fff;
    border-top: 2rpx solid #f4f4f4;
}

.nav-link {
    height: 1rpx;
}

.nav-bar {
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-flow: row;
    justify-content: space-around;
    -webkit-box-align: center;
    align-items: center;
    height: 80rpx;
    width: 100%;
    z-index: 50;
    margin-bottom: 10rpx;
    background-color: #fff;
}

.nav-bar_item {
    height: 100%;
    text-align: center;
    line-height: 77rpx;
    font-size: 28rpx;
}

.nav-bar_item text {
    display: inline-block;
    height: 65rpx;
    max-width: 152rpx;
    min-width: 90rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.nav-bar_item.clicked {
    color: #ff3b30;
    font-weight: 600;
}

.nav-bar_item.clicked text {
    border-bottom: 4rpx solid #ff3b30;
}

.fixed {
    position: fixed;
    top: 0;
    left: 0;
}

.top {
    margin-top: 80rpx;
}

.item {
    position: relative;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    width: 50%;
    background-color: white;
}

.item-block {
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    margin: 0 auto 60rpx 10rpx;
}

.item-block .head-img {
    width: 320rpx;
    height: 320rpx;
    position: relative;
    margin-left: 10rpx;
    background: #f8f8f8 url(https://static.seecsee.com/images/h5/placehold.svg) 50% no-repeat;
}

.item-block .head-img .sold-out {
    position: absolute;
    top: 70rpx;
    left: 70rpx;
}

.item-block .head-img .sold-out__con {
    display: inline-block;
    vertical-align: middle;
    width: 180rpx;
    height: 180rpx;
    border: 2rpx solid #fff;
    border-radius: 50%;
    background-color: rgba(0,0,0,0.5);
    text-align: center;
}

.item-block .head-img .sold-out__con text {
    display: block;
    color: #fff;
}

.item-block .head-img .sold-out__con .zh {
    font-size: 30rpx;
    line-height: 30rpx;
    margin-top: 64rpx;
}

.item-block .head-img .sold-out__con .en {
    font-size: 20rpx;
}

.item-block image {
    width: 100%;
    height: 100%;
}

.item-block .name-line {
    font-size: 28rpx;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    height: 80rpx;
    color: #545454;
    margin: 0 10rpx;
}

.item-block .price-view {
    display: -webkit-box;
    display: flex;
    -webkit-box-align: baseline;
    align-items: baseline;
    margin: 0 10rpx;
}

.item-block .price-line {
    color: #333;
    font-weight: 600;
    font-size: 28rpx;
    text-align: center;
}

.item-block .price-line-none {
    color: #333;
    font-weight: 600;
    font-size: 28rpx;
    text-align: center;
}

.item-block .oriprice-line {
    color: #888888;
    text-decoration: line-through;
    font-size: 20rpx;
    padding-left: 16rpx;
}

.moreBtn {
    margin: 50rpx auto;
    width: 600rpx;
    height: 80rpx;
    background-color: #ff3b30;
    text-align: center;
    border-radius: 4rpx;
}

.moreBtn text {
    font-size: 30rpx;
    line-height: 80rpx;
    color: #fff;
}

button.more {
    width: 674rpx;
    height: 88rpx;
    line-height: 80rpx;
    border-radius: 4rpx;
    margin-bottom: 60rpx;
    background-color: #fefefe;
    border: solid 4rpx #eeeeee;
    font-size: 28rpx;
}

button.more:after {
    border: none;
}