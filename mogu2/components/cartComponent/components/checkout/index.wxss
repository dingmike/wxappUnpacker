.co-header {
    border: none!important;
}

.co-item {
    width: 100%;
    height: 150rpx;
    padding-right: 32rpx;
}

.co-item:first-child {
    border-bottom: 0.5px solid #dedede;
}

.co-title {
    line-height: 40rpx;
    margin-bottom: 10rpx;
    color: #555;
    font-size: 28rpx;
}

.co-desc {
    line-height: 32rpx;
    color: #999;
    font-size: 24rpx;
}