page {
    background-color: #f4f4f4;
}

.order-container {
    background-color: #F2F2F2;
}

.order-header {
    height: 100rpx;
    padding: 0 20rpx;
    color: #FFFFFF;
    background-color: #888;
    display: -webkit-box;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.order-header-text {
    font-size: 14px;
    white-space: nowrap;
}

.order-address {
    width: 100%;
    display: -webkit-box;
    display: flex;
    background-color: #FFFFFF;
    -webkit-box-align: center;
    align-items: center;
    padding: 0 14rpx;
    margin-bottom: 0;
    line-height: 17px;
    padding-top: 15rpx;
    padding-bottom: 15rpx;
    font-size: 24rpx;
    color: #545454;
    letter-spacing: 2rpx;
}

.order-address-detail text {
    margin: 0;
    font-size: 12px;
}

.order-list {
    margin-bottom: 20rpx;
    background-color: #FFFFFF;
}

.order-list-item {
    border: 0 solid #F4F4F4;
    padding: 20rpx;
}

.order-list-item-country {
    display: -webkit-box;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.order-list-item-country image {
    width: 45rpx;
    height: 30rpx;
    margin-right: 20rpx;
}

.order-list-item-country text {
    font-size: 14px;
    white-space: nowrap;
}

.order-list-item-goods {
    background-color: #fff;
    margin-top: 20rpx;
    border-bottom: 2rpx solid #f4f4f4;
    display: -webkit-box;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    padding: 60rpx 0;
}

.order-list-item-goods .item_imgurl-container {
    display: -webkit-box;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.order-list-item-goods .item_img {
    height: 150rpx;
    width: 150rpx;
}

.order-list-item-goods .item_name_brand_attr-container {
    height: 150rpx;
    max-width: 40%;
    white-space: nowrap;
}

.order-list-item-goods .item_name_brand_attr-container .item_brand,.order-list-item-goods .item_name_brand_attr-container .item_attr {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    display: inline-block;
    width: 100%;
    font-size: 14px;
}

.order-list-item-goods .item_name_brand_attr-container .item_name,.order-list-item-goods .item_name_brand_attr-container .item_brand,.order-list-item-goods .item_name_brand_attr-container .item_attr {
    color: #999999;
    margin: 0;
    padding: 0;
    font-size: 13px;
}

.order-list-item-goods .item_fee_quantity-container {
    padding-top: 20rpx;
}

.order-list-item-goods .item_fee_quantity-container view {
    height: 40rpx;
}

.order-list-item-goods .item_fee_quantity-container .item_fee {
    text-align: right;
    float: right;
    font-weight: 700;
    font-size: 13px;
    margin-right: 20rpx;
}

.order-list-item-goods .item_fee_quantity-container .item_fee.item_status_str {
    color: #FF9A0F;
    white-space: nowrap;
}

.order-list-item-fee {
    margin-top: 30rpx;
}

.order-list-item-fee-row {
    display: -webkit-box;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    font-size: 26rpx;
    color: #545454;
}

.order-list-item-fee-row-value {
    color: #999999;
}

.order-list-item-fee-row-price {
    color: #e64340;
}

.contactButton {
    width: 180rpx;
    height: 55rpx;
    margin: 0 20rpx 0 auto;
    display: -webkit-box!important;
    display: flex!important;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    background-color: #fff;
    color: #888;
    font-size: 24rpx;
    border-radius: 4rpx;
    padding-left: 0!important;
    padding-right: 0!important;
}

.contactButton image {
    width: 35rpx;
    height: 35rpx;
    margin-bottom: 5rpx;
    margin-right: 20rpx;
}

.contactButton::after {
    content: none;
}

.view-pay {
    position: fixed;
    bottom: 0;
    width: 100%;
}

.view-big-order {
    font-size: 12px;
    color: #999;
    padding: 20rpx;
    background-color: #FFF;
    margin-bottom: 20rpx;
}

.commodityfixbtn {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 90rpx;
    text-align: center;
    line-height: 90rpx;
    background-image: -webkit-linear-gradient(-45deg,#FE3B2E,#FE3B2E);
    border: 1rpx solid #F4F4F4;
    margin-bottom: 1rpx;
}

.total_info {
    width: 40%;
}

.total_info {
    float: left;
    background: #fff;
    width: 62%;
    font-size: 14px;
    text-align: left;
    padding-left: 10px;
}

.total_info-tips {
    color: #545454;
}

.total_info-price {
    color: red;
    font-weight: 700;
}

.total_info-second {
    color: #999;
    font-size: 12px;
    float: right;
    padding-right: 15px;
}

.cancel_order {
    width: 30%;
    float: left;
    background: #fff;
    font-size: 12px;
    color: #000;
}

.pay_btn {
    color: #000;
    width: 30%;
    float: left;
    background-color: #09BB07;
    color: #fff;
}

.pay_btn-text {
    color: #FFF;
    font-size: 16px;
}

.order-item-image {
    margin-right: 6rpx;
    width: 150rpx;
    height: 150rpx;
}

.order-item-image image {
    width: 150rpx;
    height: 150rpx;
}

.order-item-name {
    line-height: 32rpx;
    letter-spacing: 2rpx;
    color: #545454;
    text-overflow: ellipsis;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.order-item-text-info {
    padding: 10rpx;
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-flex: 1;
    flex-grow: 1;
    width: 20%;
    font-size: 24rpx;
    color: #545454;
    letter-spacing: 2rpx;
}

.order-item-text-info text:last-of-type {
    margin: auto 0 0;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.order-item-price {
    padding: 15rpx 20rpx 0 0;
    font-size: 26rpx;
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: end;
    align-items: flex-end;
}

.order-item-price text:last-of-type {
    font-size: 24rpx;
}

.empty-view {
    height: 25rpx;
    background-color: #F2F2F2;
    width: 100%;
}

.location-container {
    background-color: #FFF;
    display: -webkit-box;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    margin-bottom: 20rpx;
}

.location-icon {
    height: 20px;
    width: 16px;
    margin: 20rpx;
}

.order-transport-info {
    background-color: #fff;
}

.order-transport-info .transport-title {
    font-size: 14px;
    padding: 20rpx;
}

.order-transport-info .track-h {
    display: -webkit-box;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    padding: 0 40rpx 48rpx 40rpx;
    border-bottom: 1px solid #f2f2f2;
}

.order-transport-info .track-h-item {
    font-size: 13px;
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-flex: 1;
    flex: 1;
    position: relative;
}

.order-transport-info .track-h-item:first-child:after {
    position: static;
    width: 0;
    height: 0;
}

.order-transport-info .track-h-item:after {
    content: "";
    position: absolute;
    top: 20rpx;
    right: 50%;
    width: 100%;
    height: 2rpx;
    background-color: #e5e5e5;
}

.order-transport-info .track-h-node {
    position: relative;
    border: #e5e5e5 4rpx solid;
    border-radius: 50%;
    box-sizing: border-box;
    width: 40rpx;
    height: 40rpx;
    background-color: #fff;
    z-index: 1;
    display: block;
}

.order-transport-info .active-h .track-h-node {
    border-color: #FE3B2E;
}

.order-transport-info .track-h-node::before {
    content: "";
    display: block;
    position: absolute;
    left: 50%;
    top: 50%;
    width: 0;
    height: 0;
    background-color: #ccc;
    border-top-left-radius: 999px;
    border-top-right-radius: 999px;
    border-bottom-right-radius: 999px;
    border-bottom-left-radius: 999px;
    border: 5px solid transparent;
    margin-top: -5px;
    margin-left: -5px;
}

.order-transport-info .active-h .track-h-node:before {
    background-color: #FE3B2E;
}

.order-transport-info .track-h-desc {
    text-align: center;
    margin-top: 24rpx;
    width: 60rpx;
    display: block;
    line-height: 16px;
}

.order-transport-info .track-v-next {
    display: -webkit-box;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.order-transport-info .track-v {
    font-size: 13px;
    font-weight: normal;
    color: #888;
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    font-size: 26rpx;
    padding: 12rpx 25rpx;
    position: relative;
    overflow: hidden;
}

.order-transport-info .track-v-item {
    display: -webkit-box;
    display: flex;
    position: relative;
    padding-right: 40rpx;
}

.order-transport-info .track-v-item-left {
    width: 15%;
    -webkit-box-pack: center;
    justify-content: center;
    display: -webkit-box;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    flex-shrink: 0;
    position: relative;
}

.order-transport-info .track-v-desc {
    border-bottom: 1px solid #f2f2f2;
    padding: 32rpx 0;
    height: 100%;
    width: 80%;
    display: -webkit-box;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.order-transport-info .track-v-desc-status {
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.order-transport-info .active-v .track-v-desc {
    color: #FE3B2E;
    font-weight: bold;
}

.order-transport-info .track-v-item:after {
    content: "";
    position: absolute;
    left: 48rpx;
    top: 50%;
    width: 4.5rpx;
    height: 150%;
    background-color: #ccc;
}

.order-transport-info .track-v-item:last-child .track-v-desc {
    border: 0;
}

.order-transport-info .track-v-item:last-child:after {
    background-color: #fff;
}

.order-transport-info .track-v-node {
    position: relative;
    width: 26rpx;
    height: 26rpx;
    background-color: #ccc;
    border-radius: 50%;
    flex-shrink: 0;
    box-sizing: border-box;
    display: inline-block;
    z-index: 1;
}

.order-transport-info .track-v-node:before {
    position: absolute;
    content: "";
    left: 50%;
    top: 50%;
    width: 8rpx;
    height: 8rpx;
    border-top: 2rpx solid #fff;
    border-left: 2rpx solid #fff;
    transform: rotateZ(45deg);
    transform-origin: left top;
    margin-top: -5rpx;
}

.order-transport-info .node-b-n .track-v-desc-status {
    font-weight: bold;
}

.order-transport-info .node-b-n .track-v-node {
    width: 38rpx;
    height: 38rpx;
    margin-left: 0;
}

.order-transport-info .node-b-n .track-v-node:before {
    content: "";
    width: 0;
    height: 0;
    display: none;
}

.order-transport-info .active-v .track-v-node {
    width: 40rpx;
    height: 40rpx;
    border: #fab7c1 4rpx solid;
    -webkit-border-radius: 50%;
    border-randius: 50%;
    background-color: #fff;
    margin-left: 0;
}

.order-transport-info .active-v .track-v-node:before {
    content: "";
    display: block;
    position: absolute;
    left: 51%;
    top: 56%;
    width: 0;
    height: 0;
    background-color: #FE3B2E;
    border-radius: 50%;
    border: 10rpx solid transparent;
    margin-top: -10rpx;
    margin-left: -10rpx;
    -webkit-transform: none;
}

.contact-container {
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    font-size: 24rpx;
    color: #888;
    padding: 40rpx 20rpx;
}

.contact-container .call-see {
    display: inline-block;
    padding: 0 10rpx;
    color: #FE3B2E;
}

.contact-container .contact-see {
    display: inline-block;
    padding: 0 10rpx;
    color: #FE3B2E;
}